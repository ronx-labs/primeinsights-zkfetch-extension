{
  "manifest_version": 3,
  "name": "PrimeInsights zkFetch Extension",
  "description": "Generates a proof for Amazon shoping data using zkFetch",
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": [
    "webRequest",
    "storage"
  ],
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": [
    "https://*.amazon.com/*"
  ]
}